{
  "name": "vscode-sheep",
  "displayName": "Sheep",
  "description": "VS Code Extension for Controlling Robots",
  "version": "0.0.1",
  "publisher": "MrBBot",
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onCommand:robot.run",
    "onCommand:robot.stop"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "robot.run",
        "title": "Robot: Run on Robot"
      },
      {
        "command": "robot.stop",
        "title": "Robot: Stop Robot"
      }
    ],
    "viewsContainers": {
      "activitybar": []
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
